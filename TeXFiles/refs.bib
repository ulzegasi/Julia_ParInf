doi = {10.1111/j.1461-0248.2009.01388.x},
file = {:C$\backslash$:/Users/pomatifr/Documents/DATA/Mendely\_lit/Hillebrand, Matthiessen\_Ecology letters\_2009.pdf:pdf},
issn = {1461-0248},
journal = {Ecology letters},
keywords = {Animals,Biodiversity,Biological,Climate Change,Ecology,Ecology: trends,Extinction,Humans},
month = dec,
number = {12},
pages = {1405--19},
pmid = {19849711},
title = {{Biodiversity in a complex world: consolidation and progress in functional biodiversity research.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/19849711},
volume = {12},
year = {2009}
