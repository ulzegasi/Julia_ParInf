issn = {0962-8452},
journal = {Proceedings. Biological sciences / The Royal Society},
keywords = {Animals,Biological,Insects,Models,Nonlinear Dynamics,Plankton,Plants,Seasons,Time Factors},
month = aug,
number = {1669},
pages = {2871--80},
pmid = {19474038},
title = {{Interannual variability in species composition explained as seasonally entrained chaos.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2817202\&tool=pmcentrez\&rendertype=abstract},
volume = {276},
